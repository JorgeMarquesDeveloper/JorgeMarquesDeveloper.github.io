<?php
// Conexão com o banco de dados (substitua pelas suas configurações)

include 'bd-conn.php';

// Coleta os dados do formulário
$nomeCompleto = $_POST['nomeCompleto'];
$cpf = $_POST['cpf'];
$identidadeOrgaoExp = $_POST['identidadeOrgaoExp'];
$nacionalidade = $_POST['nacionalidade'];
// Adicione os demais campos do formulário aqui...

// Prepara a instrução SQL para a inserção
$sql = "INSERT INTO Candidato (
    nomeCompleto, cpf, identidadeOrgaoExp, nacionalidade, 
    sexo, estadoCivil, filiacaoPai, filiacaoMae, dataNascimento, numDependentes, 
    nomeSocial, cep, estado, cidade, logradouro, bairro, email, telefoneContato, 
    telefoneRecados, tempoServicoMilitar, anosMilitar, mesesMilitar, diasMilitar
) 
VALUES (
    $nomeCompleto, $cpf, $identidadeOrgaoExp, $nacionalidade, 
    $_POST[sexo], $_POST[estadoCivil], $_POST[filiacaoPai], $_POST[filiacaoMae], $_POST[dataNascimento], '$_POST[numDependentes]', 
    '$_POST[nomeSocial]', '$_POST[cep]', '$_POST[estado]', '$_POST[cidade]', '$_POST[logradouro]', '$_POST[bairro]', '$_POST[email]', '$_POST[telefoneContato]', 
    '$_POST[telefoneRecados]', '$_POST[tempoServicoMilitar]', '$_POST[anosMilitar]', '$_POST[mesesMilitar]', '$_POST[diasMilitar]'
)";

// Executa a instrução SQL
if ($conexao->query($sql) === TRUE) {
    echo "Registro inserido com sucesso.";
} else {
    echo "Erro: " . $sql . "<br>" . $conexao->error;
}

// Fecha a conexão com o banco de dados
$conexao->close();
?>